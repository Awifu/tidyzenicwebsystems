{
  "name": "tidyzenic_official_saas_app",
  "version": "1.0.0",
  "description": "TidyZenic Official SaaS Application by Awifu",
  "main": "server.js",
"scripts": {
  "start": "npm run build:css && node server.js",
  "dev": "npm run build:css && nodemon server.js",
  "migrate": "node migrate.js",
  "build": "npm run build:css",
  "build:css": "tailwindcss -i ./src/styles.css -o ./public/css/styles.css --minify",
  "dev:css": "tailwindcss -i ./src/styles.css -o ./public/css/styles.css --watch"
},

  "repository": {
    "type": "git",
    "url": "git+https://github.com/Awifu/tidyzenicwebsystems.git"
  },
  "keywords": [
    "saas",
    "node",
    "express",
    "mysql",
    "tidyzenic",
    "awifu"
  ],
  "author": "Awifu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Awifu/tidyzenicwebsystems/issues"
  },
  "homepage": "https://tidyzenic.com",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql2": "^3.14.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14"
  }
}
